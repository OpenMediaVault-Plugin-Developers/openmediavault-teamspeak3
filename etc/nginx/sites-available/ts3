server {
    server_name ts3-webgui;
    root /var/www/ts3wi;
    index index.php;
    autoindex off;
    server_tokens off;
    sendfile on;
    large_client_header_buffers 4 32k;
    error_log /var/log/nginx/ts3-webgui_error.log error;
    access_log /var/log/nginx/ts3-webgui_access.log combined;
    location ~ \.php$ {
        try_files $uri = 404;
        fastcgi_split_path_info ^(.+\.php)(/.+)$;
        fastcgi_pass unix:/var/run/php5-fpm-openmediavault-webgui.sock;
        fastcgi_index index.php;
        include fastcgi_params;
    }
    listen 88;
}